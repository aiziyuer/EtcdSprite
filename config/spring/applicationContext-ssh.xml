<beans xmlns="http://www.springframework.org/schema/beans"	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"	xsi:schemaLocation="http://www.springframework.org/schema/beans	http://www.springframework.org/schema/beans/spring-beans.xsd">	<bean id="sshInfoDao" class="com.aiziyuer.app.ssh.dao.SshInfoDAOImpl">		<property name="jdbcTemplate" ref="jdbcTemplate" />		<property name="namedTemplate" ref="namedTemplate" />		<property name="sessionFactory" ref="sessionFactory" />	</bean>	<bean id="sshInfoBiz" parent="transactionProxy">		<property name="target">			<bean class="com.aiziyuer.app.ssh.biz.SshInfoBizImpl">				<property name="sshInfoDAO" ref="sshInfoDao" />			</bean>		</property>		<property name="transactionAttributes">			<props>				<!-- 本文仅简单用一种方式说明。所有方法起事务，可以修改以精细和区别性控制事务 -->				<prop key="*">PROPAGATION_REQUIRED</prop>			</props>		</property>	</bean></beans>